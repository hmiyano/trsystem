<% if admins.any? %>
  <ul class="media-list">
    <% admins.each do |admin| %>
      <li class="media">
        <div class="media-left">
          <img class="media-object img-rounded" src="/images/admin_logo.png" width="20" height="20" alt="" class="img-circle">
        </div>
    
        <div class="media-body">
          <div>
            <%= admin.name %>
          </div>
        
          <div>
            <p><%= link_to 'プロフィールをみる', admin_path(admin) %></p>
          </div>
        </div> 
      </li>
      <% end %>
  </ul>
  <%= paginate admins %>
<% end %>