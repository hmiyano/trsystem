<div class="row">
  <div class="col-xs-8">
    
    <%= form_for(@checklist) do |f| %>
      <%= render 'layouts/error_messages', model: f.object %>



      <div class="form-group">
        <%= f.label :id, 'ID' %>
        <%= f.text_field :id, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :grade, 'WG' %>
        <%= f.text_field :grade, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :chapter, 'チャプター' %>
        <%= f.text_field :chapter, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :section, 'セクション' %>
        <%= f.text_field :section, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label :content, 'トレーニング項目' %>
        <%= f.text_field :content, class: 'form-control' %>
      </div>
      
      <div class="form-group">
        <%= f.label :admin_id, 'ADMIN_ID' %>
        <%= f.text_field :admin_id, class: 'form-control' %>
      </div>

      <%= f.submit '編集完了', class: 'btn btn-primary' %>
    <% end %>

  </div>
</div>

