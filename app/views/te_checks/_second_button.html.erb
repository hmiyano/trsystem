<% if te_logged_in? %>

  <% if @trainee == current_trainee %>

    <% if current_trainee.second?(checklist) %>

      <%= form_tag(te_check_path(checklist.id), method: :delete) do %>
      <%= hidden_field_tag :checklist_id, checklist.id %>
      <%= hidden_field_tag :type, 'Second' %>
      <%= submit_tag ' チェック済', class: 'btn btn-success btn-xs' %>
      <% end %>
    <% else %>
      <%= form_tag(te_checks_path) do %>
        <%= hidden_field_tag :checklist_id, checklist.id %>
        <%= hidden_field_tag :type, 'Second' %>
        <%= submit_tag 'チェックする', class: 'btn btn-primary btn-xs' %>
      <% end %>
    <% end %>
  <% else %>
    違う人のや
  <% end %>

<% elsif tr_logged_in? %>
  <% if checklist.te_checks.find_by(type: Second, trainee_id: @trainee.id) %>
    済
    <%= checklist.te_checks.first.updated_at if checklist.te_checks.first %>
  <% else %>
  　未
  <% end %>
<% end %>